extends ../layout

block vars
  - var body_attrs = {'ng-app':'myApp'}
  - var style = '10.css'

block content
  div(layout='row')
    div(flex=33) 33%
    div(flex) (67%)
  div(layout='row',style={'margin-top': '24px'})
    div(flex=66) 66%
    div(flex) (9%)
    div(flex=25) 25%
  div(layout='column',style={'margin-top': '24px'})
    div(flex=20) 20%
    div(flex) (40%)
    div(flex) (40%)
  div(layout='column',style={'height': '200px', 'margin-top': '24px'})
    div(flex=20) 20%
    div(flex) (40%)
    div(flex) (40%)

append scripts
  script(type='text/javascript')
    :coffee-script
      angular.module 'myApp', ['ngMaterial']
