doctype html
html(lang='en')
  != partial('../_header')
    link(rel='stylesheet',href='12.css')
  body(ng-app='myApp')
    div(layout='row')
      div(flex,flex-offset=20) #1 --> 20%
    div(layout='row', style={'margin-top': '24px'})
      div(flex,flex-offset=50) #2 --> 50% (25%)
      div(flex) #3 (25%)
    div(layout='row', style={'margin-top': '24px'})
      div(flex=25,flex-offset=25) #4 --> 25% (25%)
      div(flex=50,flex-offset=10) #5 --> 10% (50%)
    != partial('../_scripts')
    script(type='text/javascript')
      :coffee-script
        angular.module 'myApp', ['ngMaterial']
